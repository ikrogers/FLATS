<%= form_for(@diet_manager) do |f| %>
  <% if @diet_manager.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@diet_manager.errors.count, "error") %> prohibited this diet_manager from being saved:</h2>

      <ul>
      <% @diet_manager.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
	<div class="form-field">
    <%= f.label :exercise_id %><br>
    <%= select_tag(:dsel, options_for_select(Diet.all.collect {|u| [u.name, u.id]}),{data:{live_search: "true"},:id=>"sel",:class => 'form-control',:placeholder=>"Click here to select all absent users. Leave blank if all present", :multiple => true, :name=>'project[sel_diet][]'}) %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
